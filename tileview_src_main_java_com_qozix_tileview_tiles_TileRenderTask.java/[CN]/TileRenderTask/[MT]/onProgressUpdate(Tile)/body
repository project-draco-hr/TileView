{
  TileCanvasViewGroup tileCanvasViewGroup=mTileManagerWeakReference.get();
  if (tileCanvasViewGroup != null) {
    if (tileCanvasViewGroup.getRenderIsCancelled()) {
      return;
    }
    if (isCancelled()) {
      return;
    }
    Tile tile=params[0];
    tileCanvasViewGroup.renderIndividualTile(tile);
  }
}
