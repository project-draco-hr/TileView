{
  Point point=new Point();
  point.x=getScrollX();
  point.y=getScrollY();
  for (  GestureListener listener : mGestureListeners) {
    listener.onScrollComplete(point);
  }
  if (mIsBeingFlung) {
    mIsBeingFlung=false;
    for (    GestureListener listener : mGestureListeners) {
      listener.onFlingComplete(point);
    }
  }
}
