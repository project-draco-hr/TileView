{
  int x=getScrollX();
  int y=getScrollY();
  for (  GestureListener listener : mGestureListeners) {
    listener.onScrollComplete(x,y);
  }
  if (mIsBeingFlung) {
    mIsBeingFlung=false;
    for (    GestureListener listener : mGestureListeners) {
      listener.onFlingComplete(x,y);
    }
  }
  invalidate();
}
