{
  boolean rendered=renderTile();
  Handler handler=getHandler();
  if (handler != null) {
    int status=rendered ? TileRenderHandler.RENDER_COMPLETE : TileRenderHandler.RENDER_INCOMPLETE;
    Message message=handler.obtainMessage(status,this);
    message.sendToTarget();
  }
  if (rendered) {
    mComplete=true;
  }
}
